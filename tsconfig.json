{
  "files": [],
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "src/**/*.ts",
    "src/**/*.vue"
  ],
  "compilerOptions": {
    "types": [
      "node"
    ],
    "module": "esnext",
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "baseUrl": "./",
    "paths": {
      "Components/*": ["src/components/*"],
      "Scripts/*": ["src/scripts/*"],
      "Assets/*": ["src/assets/*"],
      "Types/*": ["src/types/*"]
    }
  }
}
